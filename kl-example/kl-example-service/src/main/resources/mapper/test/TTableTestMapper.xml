<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ewp.examples.service.data.test.mapper.TTableTestMapper">

    <select id="selectTableBypage" resultType="com.ewp.examples.service.data.test.entity.TTableTestEntity">
        SELECT * FROM t_table_test
        <if test="phone_model != null">
            where phone_model = #{phone_model}
        </if>
    </select>
</mapper>
